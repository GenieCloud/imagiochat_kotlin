<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.imagiodevco.imagiochat_kotlin">

    <application
        android:allowBackup="true"
        android:icon="@drawable/imagio_logo_xhdpi"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".messages.ChatLogActivity"><meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".messages.LatestMessageActivity" /></activity>
        <activity android:name=".messages.LatestMessageActivity">

        </activity>
        <activity android:name=".registerlogin.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".registerlogin.RegisterActivity"></activity>
        <activity android:name=".messages.NewMessageActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".messages.LatestMessageActivity" />
        </activity>
    </application>

</manifest>